# Database
DATABASE_URL=postgresql:xxx

# Embeddings
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=text-embedding-3-small
OPENAI_API_KEY=sk-xxxx

# Summarization
SUMMARIZE_MODEL=gpt-4o
CHUNK_SIZE_CHARS=2000
CHUNK_OVERLAP_CHARS=200
MAX_CONCURRENCY=2
RETRY_MAX=3
OPENAI_TIMEOUT_SEC=60

# Optional logging
LOG_LEVEL=INFO
